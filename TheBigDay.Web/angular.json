{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "consumer": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/consumer",
            "sourceRoot": "projects/consumer/src",
            "prefix": "consumer",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/consumer",
                        "index": "projects/consumer/src/index.html",
                        "main": "projects/consumer/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/consumer/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/consumer/src/favicon.ico",
                            "projects/consumer/src/assets"
                        ],
                        "styles": [
                            "projects/consumer/src/styles.scss"
                        ],
                        "scripts": [],
                        "fileReplacements": [{
                            "replace": "projects/consumer/src/environments/environment.ts",
                            "with": "projects/consumer/src/environments/environment.prod.ts"
                        },
                        {
                            "replace": "projects/common/src/environments/environment.ts",
                            "with": "projects/common/src/environments/environment.prod.ts"
                        }]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                          "buildTarget": "consumer:build:production"
                        },
                        "development": {
                          "buildTarget": "consumer:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                      "buildTarget": "consumer:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/consumer/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/consumer/src/favicon.ico",
                            "projects/consumer/src/assets"
                        ],
                        "styles": [
                            "projects/consumer/src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        },
        "common": {
            "projectType": "library",
            "root": "projects/common",
            "sourceRoot": "projects/common/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "projects/common/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/common/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "projects/common/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "tsConfig": "projects/common/tsconfig.spec.json",
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ]
                    }
                }
            }
        },
        "vendors": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/vendors",
            "sourceRoot": "projects/vendors/src",
            "prefix": "vendors",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/vendors",
                        "index": "projects/vendors/src/index.html",
                        "main": "projects/vendors/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/vendors/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/vendors/src/favicon.ico",
                            "projects/vendors/src/assets"
                        ],
                        "styles": [
                            "projects/vendors/src/styles.scss",
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
                            "node_modules/primeng/resources/primeng.min.css"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": ["node_modules/"]
                        },
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [{
                                "replace": "projects/vendors/src/environments/environment.ts",
                                "with": "projects/vendors/src/environments/environment.prod.ts"
                            }, {
                                "replace": "projects/common/src/environments/environment.ts",
                                "with": "projects/common/src/environments/environment.prod.ts"
                            }]
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                          "buildTarget": "vendors:build:production"
                        },
                        "development": {
                          "buildTarget": "vendors:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                      "buildTarget": "vendors:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/vendors/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/vendors/src/favicon.ico",
                            "projects/vendors/src/assets"
                        ],
                        "styles": [
                            "projects/vendors/src/styles.scss",
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
                            "node_modules/primeng/resources/primeng.min.css"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": ["node_modules/"]
                        },
                        "scripts": []
                    }
                }
            }
        },
        "test": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/test",
            "sourceRoot": "projects/test/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/test",
                        "index": "projects/test/src/index.html",
                        "main": "projects/test/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/test/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/test/src/favicon.ico",
                            "projects/test/src/assets"
                        ],
                        "styles": [
                            "projects/test/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                          "buildTarget": "test:build:production"
                        },
                        "development": {
                          "buildTarget": "test:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                      "buildTarget": "test:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/test/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/test/src/favicon.ico",
                            "projects/test/src/assets"
                        ],
                        "styles": [
                            "projects/test/src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        },
        "administration": {
          "projectType": "application",
          "schematics": {
            "@schematics/angular:component": {
              "style": "scss"
            }
          },
          "root": "projects/administration",
          "sourceRoot": "projects/administration/src",
          "prefix": "administration",
          "architect": {
            "build": {
              "builder": "@angular-devkit/build-angular:browser",
              "options": {
                "outputPath": "dist/administration",
                "index": "projects/administration/src/index.html",
                "main": "projects/administration/src/main.ts",
                "polyfills": [
                  "zone.js"
                ],
                "tsConfig": "projects/administration/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                  "projects/administration/src/favicon.ico",
                  "projects/administration/src/assets"
                ],
                "styles": [
                  "projects/administration/src/styles.scss",
                    "node_modules/primeicons/primeicons.css",
                    "node_modules/primeng/resources/themes/lara-dark-purple/theme.css",
                    "node_modules/primeng/resources/primeng.min.css"
                ],
                "scripts": []
              },
              "configurations": {
                "production": {
                  "budgets": [
                    {
                      "type": "initial",
                      "maximumWarning": "500kb",
                      "maximumError": "2mb"
                    },
                    {
                      "type": "anyComponentStyle",
                      "maximumWarning": "2kb",
                      "maximumError": "4kb"
                    }
                  ],
                  "outputHashing": "all",
                    "fileReplacements": [{
                    "replace": "projects/administration/src/environments/environment.ts",
                    "with": "projects/administration/src/environments/environment.prod.ts"
                    },
                    {
                        "replace": "projects/common/src/environments/environment.ts",
                        "with": "projects/common/src/environments/environment.prod.ts"
                    }]
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
              },
              "defaultConfiguration": "production"
            },
            "serve": {
              "builder": "@angular-devkit/build-angular:dev-server",
              "configurations": {
                "production": {
                  "buildTarget": "administration:build:production"
                },
                "development": {
                  "buildTarget": "administration:build:development"
                }
              },
              "defaultConfiguration": "development"
            },
            "extract-i18n": {
              "builder": "@angular-devkit/build-angular:extract-i18n",
              "options": {
                "buildTarget": "administration:build"
              }
            },
            "test": {
              "builder": "@angular-devkit/build-angular:karma",
              "options": {
                "polyfills": [
                  "zone.js",
                  "zone.js/testing"
                ],
                "tsConfig": "projects/administration/tsconfig.spec.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                  "projects/administration/src/favicon.ico",
                  "projects/administration/src/assets"
                ],
                "styles": [
                    "projects/administration/src/styles.scss",
                    "node_modules/primeicons/primeicons.css",
                    "node_modules/primeng/resources/themes/lara-dark-purple/theme.css",
                    "node_modules/primeng/resources/primeng.min.css"
                ],
                  "stylePreprocessorOptions": {
                      "includePaths": ["node_modules/"]
                  },
                "scripts": []
              }
            }
          }
        }
    },
    "cli": {
        "analytics": false
    }
}
