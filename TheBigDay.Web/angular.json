        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "vendors:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/vendors/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/vendors/src/favicon.ico",
              "projects/vendors/src/assets"
            ],
            "styles": [
              "projects/vendors/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
