{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "vendor": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/vendor",
      "sourceRoot": "projects/vendor/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "preserveSymlinks": true,
            "outputPath": "dist/vendor",
            "index": "projects/vendor/src/index.html",
            "main": "projects/vendor/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/vendor/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/vendor/src/favicon.ico",
              "projects/vendor/src/assets"
            ],
            "styles": [
              "projects/vendor/src/styles.scss"
            ],
            "scripts": [],
            "fileReplacements": [{
              "replace": "src/environments/environment.ts",
              "with": "src/environments/environment.prod.ts"
            }]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "vendor:build:production"
            },
            "development": {
              "browserTarget": "vendor:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "vendor:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/vendor/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/vendor/src/favicon.ico",
              "projects/vendor/src/assets"
            ],
            "styles": [
              "projects/vendor/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "consumer": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/consumer",
      "sourceRoot": "projects/consumer/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/consumer",
            "index": "projects/consumer/src/index.html",
            "main": "projects/consumer/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/consumer/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/consumer/src/favicon.ico",
              "projects/consumer/src/assets"
            ],
            "styles": [
              "projects/consumer/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "consumer:build:production"
            },
            "development": {
              "browserTarget": "consumer:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "consumer:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
